["custom"] = "aura_env.collector = select(3, GetAchievementCriteriaInfo(41038, 1)) --later\n\naura_env.data = { \n    [1] = {\n        npc = \"|cFFffd100Intro/Guests|r\",\n        quests = {\n            [1] = {84254, \"|cFFFFFFFF3|r|T4638504:0|t|A:Crosshair_Quest_64:0:0|a\", }, --Meet and Greet\n            [2] = {84489, \"|cFFFFFFFF5|r|T4638504:0|t|A:Crosshair_Quest_64:0:0|a\"}, --Shopping Squee!\n            [3] = {84735, \"|cFFFFFFFF5|r|T4638504:0|t|A:Crosshair_Quest_64:0:0|a\"}, --Truth of the Dark Irons\n            [4] = {84144, \"|cFFFFFFFF2|r|T4638504:0|t|A:Crosshair_Quest_64:0:0|a\"}, --Tat Big Meanie\n            [5] = {84363, \"|cFFFFFFFF2|r|T4638504:0|t|A:Crosshair_Quest_64:0:0|a\"}, --Tweasure Hunt\n            [6] = {84521, \"|cFFFFFFFF2|r|T4638504:0|t|A:Crosshair_Quest_64:0:0|a\"}, --Thoughtful Pursuits\n        },\n        enabled = aura_env.config.intro,\n    },\n    [2] = {\n        npc = \"|cFFffd100Celebration Crates|r\",\n        quests = {\n            [1] = {83794, \"|cFFFFFFFF2|r|T4638504:0|t|T132761:0|t\"}, --Soggy Celebration Crate\n            [2] = {85574, \"|cFFFFFFFF2|r|T4638504:0|t|T132761:0|t\"}, --Hazy Celebration Crate\n        },\n        enabled = aura_env.config.crate,\n    },\n    [3] = {\n        npc = \"|cFFffd100Tanaris|r\",\n        quests = {\n            [1] = {84616, \"|cFFFFFFFF3|r|T4638504:0|t|A:UI-QuestPoiRecurring-QuestBang:0:0|a\"}, --Celebrate Good Fun!\n            [2] = {60215, \"|cFFFFFFFF3|r|T4638504:0|t|A:UI-QuestPoiRecurring-QuestBang:0:0|a\"}, --Timely Gate Crashers\n            [3] = {82783, \"|cFFFFFFFF3|r|T4638504:0|t|A:UI-QuestPoiRecurring-QuestBang:0:0|a\"}, --Chromie's Codex\n            [4] = {{85947, 83274}, \"|cFFFFFFFF3|r|T4638504:0|t|A:UI-QuestPoiRecurring-QuestBang:0:0|a\",\n                C_QuestLog.GetTitleForQuestID(83274) or \"An Original Path Through Time\"}, --An Original Path Through Time\n            [5] = {82817, \"|cFFFFFFFF3|r|T4638504:0|t|A:UI-QuestPoiRecurring-QuestBang:0:0|a\"}, --Disturbance Detected: Blackrock Depths\n        },\n        enabled = aura_env.config.tanaris,\n    },\n    [4] = {\n        npc = \"|cFFffd100Khaz Algar|r\",\n        quests = {\n            [1] = {83240, \"|cFFFFFFFF1|r|T4638504:0|t|T6029029:0|t\"}, --The Theater Troupe\n            [2] = {83333, \"|cFFFFFFFF1|r|T4638504:0|t|T6029027:0|t\"}, --Gearing Up for Trouble\n            [3] = {82946, \"|cFFFFFFFF1|r|T4638504:0|t|T6029027:0|t\"}, --Rollin' Down in the Deeps\n            [4] = {76586, \"|cFFFFFFFF1|r|T4638504:0|t|T6029028:0|t\"}, --Spreading the Light\n            --Eyes of the Weaver\n            --Blade of the General\n            --Hand of the Vizier\n            [5] = {{80670,80671,80672}, \"|cFFFFFFFF1|r|T4638504:0|t|T6029030:0|t\",\n                C_QuestLog.GetTitleForQuestID(80592) or \"The Severed Threads Pact\"},\n            --Activation Protocol\n            --Aggregation of Horrors\n            --Shurrai, Atrocity of the Undersea\n            --Orta, the Broken Mountain\n            [6] = {{81630,82653,81653,81624}, \"|cFFFFFFFF1|r|T4638504:0|t|A:vignettekillboss:0:0|a\", \"World Boss\"},\n            [7] = {false, \"|cFFFFFFFF3|r|T4638504:0|t|A:Raid:0:0|a\", EJ_GetEncounterInfo(2602) or \"Queen Ansurek\"}, --Queen Ansurek\n        },\n        enabled = aura_env.config.khaz,\n    },\n    [5] = {\n        npc = \"|cFFffd100Meta/Assignmets|r |cFF00FFFF API return!|nSome quests might be not active!|r\",\n        quests = {\n        },\n        enabled = aura_env.config.meta,\n    },\n    [6] = {\n        npc = \"|cFFffd100PvP (Tanaris/Khaz Algar) |cFF00FFFFAPI! Check rewards|nSome quests might be not active/reward!|r\",\n        quests = {\n            [1] = {57300, \"|cFFFFFFFF3|r|T4638504:0|t|A:UI-QuestPoiRecurring-QuestBang:0:0|a\"}, --Soldier of Time\n        },\n        enabled = aura_env.config.pvp,\n    },\n}\n\naura_env.getReward = function(questID)\n    if not questID then return false end\n    local data = C_QuestLog.GetQuestRewardCurrencies(questID)\n    if data and #data > 0 then\n        for _, info in ipairs(data) do\n            if info.currencyID == 3100 then\n                return true\n            end\n        end        \n    end\nend\n\n--------------------CURRENT META QUEST--------------------\n\n--C_TaskQuest.IsActive(questID) weird return\nlocal metaQuests = {\n    82449, --The Call of the Worldsoul\n    82482, --Worldsoul: Snuffling\n    82516, --Worldsoul: Forging a Pact\n    82483, --Worldsoul: Spreading the Light\n    82453, --Worldsoul: Encore!\n    82489, --Worldsoul: The Dawnbreaker\n    82659, --Worldsoul: Nerub-ar Palace\n    82678, --Archives: The First Disc\n    82679, --Archives: Seeking History\n    82490, --Worldsoul: Priory of the Sacred Flame\n    82491, --Worldsoul: Ara-Kara, City of Echoes\n    82492, --Worldsoul: City of Threads\n    82493, --Worldsoul: The Dawnbreaker\n    82494, --Worldsoul: Ara-Kara, City of Echoes\n    82496, --Worldsoul: City of Threads\n    82497, --Worldsoul: The Stonevault\n    82498, --Worldsoul: Darkflame Cleft\n    82499, --Worldsoul: Priory of the Sacred Flame\n    82500, --Worldsoul: The Rookery\n    82501, --Worldsoul: The Dawnbreaker\n    82502, --Worldsoul: Ara-Kara, City of Echoes\n    82503, --Worldsoul: Cinderbrew Meadery\n    82504, --Worldsoul: City of Threads\n    82505, --Worldsoul: The Stonevault\n    82506, --Worldsoul: Darkflame Cleft\n    82507, --Worldsoul: Priory of the Sacred Flame\n    82508, --Worldsoul: The Rookery\n    82509, --Worldsoul: Nerub-ar Palace\n    82510, --Worldsoul: Nerub-ar Palace\n    82511, --Worldsoul: Awakening Machine\n    82512, --Worldsoul: World Boss\n    82488, --Worldsoul: Darkflame Cleft\n    82487, --Worldsoul: The Stonevault\n    82486, --Worldsoul: The Rookery\n    82485, --Worldsoul: Cinderbrew Meadery\n    82452, --Worldsoul: World Quests\n    82495, --Worldsoul: Cinderbrew Meadery\n    82706, --Delves: Khaz Algar Research\n    82707, --Delves: Earthen Defense\n    82708, --Delves: Nerubian Menace\n    82709, --Delves: Percussive Archaeology\n    82710, --Delves: Empire-ical Exploration\n    82711, --Delves: Lost and Found\n    82712, --Delves: Trouble Up and Down Khaz Algar\n    82746, --Delves: Breaking Tough to Loot Stuff\n}\n\n--------------------CURRENT PVP QUESTS--------------------\n\nlocal pvpQuests = {\n    47148, --Something Different ??? returns currency when not active\n    81793, --Sparks of War: Isle of Dorn\n    81794, --Sparks of War: The Ringing Deeps\n    81795, --Sparks of War: Hallowfall\n    81796, --Sparks of War: Azj-Kahet\n    80184, --Preserving in Battle\n    80185, --Preserving Solo\n    80186, --Preserving in War\n    80187, --Preserving in Skirmishes\n    80188, --Preserving in Arenas\n    80189, --Preserving Teamwork\n} \n\n-----------------CURRENT ASSIGNMENT QUESTS----------------\n\nlocal saQuests = {\n    --3 same quest different location?\n    82355, --Special Assignment: Cinderbee Surge\n    81649, --Special Assignment: Titanic Resurgence\n    81647, --Special Assignment: Titanic Resurgence\n    81650, --Special Assignment: Titanic Resurgence\n    81691, --Special Assignment: Shadows Below\n    83229, --Special Assignment: When the Deeps Stir\n    82852, --Special Assignment: Lynx Rescue\n    82787, --Special Assignment: Rise of the Colossals\n    82414, --Special Assignment: A Pound of Cure\n    82531, --Special Assignment: Bombs from Behind\n    \n}\n\n-----------------CURRENT ASSIGNMENT QUESTS----------------\nlocal function requestData()\n    --Meta\n    for _, questID in ipairs(metaQuests) do\n        local token = aura_env.getReward(questID)\n        if token then\n            local added\n            for _, info in ipairs(aura_env.data[5].quests) do\n                if info and info[1] == questID then\n                    added = true\n                    break\n                end\n            end\n            if not added then\n                table.insert(aura_env.data[5].quests, {questID, \"|cFFFFFFFF2|r|T4638504:0|t|A:UI-QuestPoiRecurring-QuestBang:0:0|a\", nil, true})\n            end\n        end               \n    end\n    --Special Assignments\n    for _, questID in ipairs(saQuests) do\n        local token = aura_env.getReward(questID)\n        if token then\n            local added\n            for _, info in ipairs(aura_env.data[5].quests) do\n                if info and info[1] == questID then\n                    added = true\n                    break\n                end\n            end\n            if not added then\n                table.insert(aura_env.data[5].quests, {questID, \"|cFFFFFFFF1|r|T4638504:0|t|A:UI-QuestPoiRecurring-QuestBang:0:0|a\", nil, true})\n            end\n        end               \n    end\n    --PvP\n    for _, questID in ipairs(pvpQuests) do\n        local token = aura_env.getReward(questID)\n        if token then\n            local added\n            for _, info in ipairs(aura_env.data[6].quests) do\n                if info and info[1] == questID then\n                    added = true\n                    break\n                end\n            end\n            if not added then\n                local description = format(\"|cFFFFFFFF%d|r|T4638504:0|t|A:UI-QuestPoiRecurring-QuestBang:0:0|a\", questID == 47148 and 2 or 1)\n                table.insert(aura_env.data[6].quests, {questID, description, nil, true})\n            end\n        end               \n    end\nend\nrequestData()\n\n--------------MODIFIER_STATE_CHANGED TRIGGER--------------\n\nlocal modKeys = {\n    [1] = \"LCTRL\",\n    [2] = \"RCTRL\",\n    [3] = \"LSHIFT\",\n    [4] = \"RSHIFT\",\n    [5] = \"LALT\",\n    [6] = \"RALT\",\n}\naura_env.key = modKeys[aura_env.config.key]\n\n---------------------WEEKLY RAID BOSS---------------------\n\naura_env.instance = function()\n    for i=14, 17 do\n        local queen = C_RaidLocks.IsEncounterComplete(2657, 2922, i)\n        if queen then\n            return true\n        end\n    end\n    return C_QuestLog.IsQuestFlaggedCompletedOnAccount(84597) or C_QuestLog.IsQuestFlaggedCompletedOnAccount(84276)\nend\n\n----------------------CACHE ON LOGIN----------------------\n\nlocal questTable = {\n    --Meta\n    [82449] = true, --The Call of the Worldsoul\n    [82482] = true, --Worldsoul: Snuffling\n    [82516] = true, --Worldsoul: Forging a Pact\n    [82483] = true, --Worldsoul: Spreading the Light\n    [82453] = true, --Worldsoul: Encore!\n    [82489] = true, --Worldsoul: The Dawnbreaker\n    [82659] = true, --Worldsoul: Nerub-ar Palace\n    [82678] = true, --Archives: The First Disc\n    [82679] = true, --Archives: Seeking History\n    [82490] = true, --Worldsoul: Priory of the Sacred Flame\n    [82491] = true, --Worldsoul: Ara-Kara, City of Echoes\n    [82492] = true, --Worldsoul: City of Threads\n    [82493] = true, --Worldsoul: The Dawnbreaker\n    [82494] = true, --Worldsoul: Ara-Kara, City of Echoes\n    [82496] = true, --Worldsoul: City of Threads\n    [82497] = true, --Worldsoul: The Stonevault\n    [82498] = true, --Worldsoul: Darkflame Cleft\n    [82499] = true, --Worldsoul: Priory of the Sacred Flame\n    [82500] = true, --Worldsoul: The Rookery\n    [82501] = true, --Worldsoul: The Dawnbreaker\n    [82502] = true, --Worldsoul: Ara-Kara, City of Echoes\n    [82503] = true, --Worldsoul: Cinderbrew Meadery\n    [82504] = true, --Worldsoul: City of Threads\n    [82505] = true, --Worldsoul: The Stonevault\n    [82506] = true, --Worldsoul: Darkflame Cleft\n    [82507] = true, --Worldsoul: Priory of the Sacred Flame\n    [82508] = true, --Worldsoul: The Rookery\n    [82509] = true, --Worldsoul: Nerub-ar Palace\n    [82510] = true, --Worldsoul: Nerub-ar Palace\n    [82511] = true, --Worldsoul: Awakening Machine\n    [82512] = true, --Worldsoul: World Boss\n    [82488] = true, --Worldsoul: Darkflame Cleft\n    [82487] = true, --Worldsoul: The Stonevault\n    [82486] = true, --Worldsoul: The Rookery\n    [82485] = true, --Worldsoul: Cinderbrew Meadery\n    [82452] = true, --Worldsoul: World Quests\n    [82495] = true, --Worldsoul: Cinderbrew Meadery\n    [82706] = true, --Delves: Khaz Algar Research\n    [82707] = true, --Delves: Earthen Defense\n    [82708] = true, --Delves: Nerubian Menace\n    [82709] = true, --Delves: Percussive Archaeology\n    [82710] = true, --Delves: Empire-ical Exploration\n    [82711] = true, --Delves: Lost and Found\n    [82712] = true, --Delves: Trouble Up and Down Khaz Algar\n    [82746] = true, --Delves: Breaking Tough to Loot Stuff\n    --Intro\n    [84254] = true,\n    [84489] = true,\n    [84735] = true,\n    [84144] = true,\n    [84363] = true,\n    [84521] = true,\n    --Weekly\n    [84616] = true,\n    [60215] = true,\n    [82783] = true,\n    [83274] = true,\n    [82817] = true,\n    [83240] = true,\n    [83333] = true,\n    [82946] = true,\n    [76586] = true,\n    --Special Assignment: Cinderbee Surge/Special Assignment: Titanic Resurgence\n    --3 same quest different location?\n    [82355] = true,\n    [81649] = true,\n    [81647] = true,\n    [81650] = true,\n    --Special Assignment: Shadows Below/Special Assignment: When the Deeps Stir\n    [81691] = true,\n    [83229] = true,\n    --Special Assignment: Lynx Rescue/Special Assignment: Rise of the Colossals\n    [82852] = true,\n    [82787] = true,\n    --Special Assignment: A Pound of Cure/Special Assignment: Bombs from Behind\n    [82414] = true,\n    [82531] = true,\n    --Severed Threads\n    [80670] = true,\n    [80671] = true,\n    [80672] = true,\n    --World Boss\n    [81630] = true,\n    [82653] = true,\n    [81653] = true,\n    [81624] = true,\n    --PvP\n    [57300] = true,\n    [47148] = true,\n    [81793] = true,\n    [81794] = true,\n    [81795] = true,\n    [81796] = true,\n    [80184] = true,\n    [80185] = true,\n    [80186] = true,\n    [80187] = true,\n    [80188] = true,\n    [80189] = true,\n}\n\nlocal questCache = function()\n    for questID in pairs(questTable) do\n        local cache = C_QuestLog.GetQuestObjectives(questID)\n        cache = C_TaskQuest.RequestPreloadRewardData(questID)\n        cache = C_QuestLog.GetQuestRewardCurrencies(questID)\n    end\n    WeakAuras.ScanEvents(\"QUEST_LOG_UPDATE\")\nend\n\nif not aura_env.region.OnLogin then\n    aura_env.region.OnLogin = true\n    \n    C_Timer.NewTicker(2, questCache, 5)\n    \n    local aura_env = aura_env\n    C_Timer.NewTicker(2, requestData, 5)\nend\n\n-----------------------STATIC POPUP-----------------------\n\naura_env.regions = {}\n\nlocal locale = GetLocale()\nlocal localization = {\n    [\"ruRU\"] = \"https://www.wowhead.com/ru/quest=%d\",\n    [\"enGB\"] = \"https://www.wowhead.com/quest=%d\",\n    [\"enUS\"] = \"https://www.wowhead.com/quest=%d\",\n    [\"frFR\"] = \"https://www.wowhead.com/fr/quest=%d\",\n    [\"deDE\"] = \"https://www.wowhead.com/de/quest=%d\",\n    [\"koKR\"] = \"https://www.wowhead.com/ko/quest=%d\",\n    [\"zhCN\"] = \"https://www.wowhead.com/cn/quest=%d\",\n    [\"zhTW\"] = \"https://www.wowhead.com/cn/quest=%d\",\n    [\"esES\"] = \"https://www.wowhead.com/es/quest=%d\",\n    [\"esMX\"] = \"https://www.wowhead.com/es/quest=%d\",\n    [\"ptBR\"] = \"https://www.wowhead.com/pt/quest=%d\",\n    [\"itIT\"] = \"https://www.wowhead.com/it/quest=%d\",\n}\nlocal pattern = localization[locale] or localization[\"enGB\"]\n\nStaticPopupDialogs[\"TRANSLIT_20TH\"] = { --Questie analog\n    text = \"WOWHEAD LINK\",\n    button2 = \"OK\",\n    hasEditBox = true,\n    editBoxWidth = 280,\n    EditBoxOnEnterPressed = function(self)\n        self:GetParent():Hide()\n    end,\n    EditBoxOnEscapePressed = function(self)\n        self:GetParent():Hide()\n    end,\n    OnShow = function (self, data)\n        local questID, questName = self.text.text_arg1, self.text.text_arg2\n        if questID then\n            local link = format(pattern, questID)\n            self.editBox:SetText(link)\n            self.editBox:SetFocus()\n            self.editBox:HighlightText()\n            local header = format(\"|A:QuestTurnin:0:0|a %s\", questName)\n            self.text:SetText(header)\n        end\n        \n        self.editBox:SetScript(\"OnKeyDown\", function(_, key)\n                if key == \"C\" and IsControlKeyDown() then\n                    C_Timer.After(0.1, function()\n                            self.editBox:GetParent():Hide()\n                            ActionStatus:DisplayMessage(\"Copied!\", true)\n                    end)\n                end\n        end)\n    end,\n    \n    whileDead = true,\n    hideOnEscape = true,\n}",
